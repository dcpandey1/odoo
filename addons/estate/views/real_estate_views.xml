<odoo>
  <data>
    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="estate.action_window">
      <field name="name">estate</field>
      <field name="res_model">real.estate</field>
      <field name="view_mode">list,form</field>
    </record>

    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="estate.list">
      <field name="name">Estate List</field>
      <field name="model">real.estate</field>
      <field name="arch" type="xml">
        <list>
          <field name="name"/>
          <field name="bedrooms"/>
          <field name="furnuishstatus"/>
          <field name="type_id"/>
          <field name="tag_ids"/>
        </list>
      </field>
    </record>


        <!-- Top menu item -->

    <menuitem name="Estate Properties" id="estate.menu_root"/>

    <!-- menu categories -->

    <menuitem name="Estate" id="estate.menu_1" parent="estate.menu_root"/>

    <!-- actions -->

    <menuitem name="Properties" id="estate.menu_1_list" parent="estate.menu_1"
              action="estate.action_window"/>





    <!-- server action to the one above -->

    <!-- <record model="ir.actions.server" id="estate.action_server">
